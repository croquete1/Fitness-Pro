{
  "name": "hms",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "postinstall": "node scripts/check-duplicate-routes.cjs",
    "prebuild": "node scripts/check-duplicate-routes.cjs && node scripts/verify-manifest.cjs && node scripts/verify-sidebar-routes.cjs && node scripts/find-invalid-catchall.cjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "npm run typecheck && npm run build",
    "find:bad-session": "rg -n \"\\bsession\\?\\.user\" src || true",
    "find:prisma": "rg -n \"@prisma/client|\\bprisma\\b\" src || true",
    "find:route-type": "rg -n \"type\\s+Route\\b|: Route\\b\" src || true",
    "find:supabase-browser": "rg -n \"createBrowserClient|supabaseBrowser\\(\" src || true",
    "find:tables": "rg -n \"plan_day_items|plan_day_exercises\" src || true",
    "doctor:mui": "npm ls @emotion/react @emotion/styled @emotion/cache @emotion/server @mui/material @mui/system @mui/utils @mui/styled-engine @mui/x-data-grid || echo ok",
    "neo:audit": "node scripts/neo-theme-audit.mjs",
    "prepare": "husky",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "6.1.2",
    "@mui/material": "6.1.2",
    "@mui/material-nextjs": "7.3.3",
    "@mui/styled-engine": "6.1.2",
    "@mui/system": "6.1.2",
    "@mui/utils": "6.1.2",
    "@mui/x-data-grid": "7.8.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.2.10",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-auth": "4.24.11",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "papaparse": "^5.4.1",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.5",
    "web-push": "^3.6.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.1",
    "@types/react": "18.2.66",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "typescript": "5.4.5"
  },
  "overrides": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "6.1.2",
    "@mui/material": "6.1.2",
    "@mui/styled-engine": "6.1.2",
    "@mui/system": "6.1.2",
    "@mui/utils": "6.1.2",
    "@mui/x-data-grid": "7.8.0"
  }
}
