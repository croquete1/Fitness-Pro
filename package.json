{
  "name": "fitness-pro",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "postinstall": "node scripts/check-duplicate-routes.cjs",
    "prebuild": "node scripts/check-duplicate-routes.cjs && node scripts/verify-manifest.cjs && node scripts/verify-sidebar.cjs && node scripts/find-invalid-catchall.cjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "next lint",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run build",
    "find:bad-session": "rg -n \"\\bsession\\?\\.user\" src || true",
    "find:prisma": "rg -n \"@prisma/client|\\bprisma\\b\" src || true",
    "find:route-type": "rg -n \"type\\s+Route\\b|: Route\\b\" src || true",
    "find:supabase-browser": "rg -n \"createBrowserClient|supabaseBrowser\\(\" src || true",
    "find:bad-imports": "rg -n \"requireUserGuard|requirePtOrAdminGuard|isGuardErr\" src || true",
    "find:tables": "rg -n \"plan_day_items|plan_day_exercises\" src || true"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.539.0",
    "next": "14.2.5",
    "next-auth": "4.24.11",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.5",
    "web-push": "^3.6.7",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/react": "18.2.66",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.5",
    "typescript": "5.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5"
  }
}
